<?xml version="1.0"?>
<extension key="uk.co.compucorp.civicrm.prospect" type="module">
  <downloadUrl>https://github.com/compucorp/uk.co.compucorp.civicrm.prospect/master</downloadUrl>
  <file>prospect</file>
  <name>CiviProspect</name>
  <description>Extends standard Case component with Prospect functionality</description>
  <license>AGPL-3.0</license>
  <maintainer>
